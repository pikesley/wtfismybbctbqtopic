<div id='content'>
  <h1 id='title'></h1>
  <p id='topic'></p>
  <a id='generate' href='/'></a>
</div>

<script>
$.getJSON(document.URL, function(json) {
  $('#title').text(getRandom(json['headings']))
  $('#topic').text(template(json))
  $('#generate').text(getRandom(json['responses']))
})
</script>
